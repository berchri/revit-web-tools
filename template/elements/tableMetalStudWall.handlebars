<table class="{{tableStyle}}" data-id="{{eNameJS}}">
    <thead class="{{theadStyle}}">
        <tr>
            <th scope="col">#</th>
            <th scope="col">Name</th>
            <th scope="col">Typ</th>
            <th scope="col">Dicke</th>
            <th scope="col">CW Profil</th>
            <th scope="col">Beplankung</th>
            <th scope="col">Spezial</th>
            <th scope="col">Anzahl</th>
            <th scope="col">Typ-Kommentar</th>
            <th scope="col">-</th>
            <th scope="col">-</th>
        </tr>
    </thead>
    <tbody class="text-center align-middle">
        {{#each typesCollection}}
        <tr>
            <th scope="row">{{id}}</th>
            <td>{{Name}}</td>
            <td>{{LV_39_Typ}}</td>
            <td>{{Q_Dicke}}</td>
            <td>{{LV_39_CW_Profil}}</td>
            <td>{{LV_39_Beplankung}}</td>
            <td>{{LV_39_Beplankung_Spezial}}</td>
            <td>{{LV_39_Beplankung_Spezial_Anzahl}}</td>
            <td>{{R_Typ_Kommentar}}</td>
            <td><a href="#" id="tr-delete" class="tr-delete" data-trID="{{@index}}" data-id="{{eNameJS}}">
                    {{{iconDelete}}}
                </a></td>
            <td><a href="#" id="tr-edit" class="tr-edit" data-trID="{{@index}}" data-id="{{eNameJS}}">
                    {{{iconEdit}}}
                </a></td>
        </tr>
        {{/each}}
    </tbody>
</table>