<table class="{{tableStyle}}" data-id="{{eNameJS}}">
    <thead class="{{theadStyle}}">
        <tr>
            <th scope="col">#</th>
            <th scope="col">Name</th>
            <th scope="col">Dicke</th>
            <th scope="col">Typ</th>
            <th scope="col">Kurztext</th>
            <th scope="col">-</th>
            <th scope="col">-</th>
        </tr>
    </thead>
    <tbody class="text-center align-middle">
        {{#each typesCollection}}
            <tr>
                <th scope="row">{{id}}</th>
                <td>{{Name}}</td>
                <td>{{Q_Dicke}}</td>
                <td>{{LV_39_Typ_Frei}}</td>
                <td>{{LV_Kurztext}}</td>
                <td><a href="#" id="tr-delete" class="tr-delete" data-trID="{{@index}}" data-id="{{eNameJS}}">
                        {{{iconDelete}}}
                    </a></td>
                <td><a href="#" id="tr-edit" class="tr-edit" data-trID="{{@index}}" data-id="{{eNameJS}}">
                        {{{iconEdit}}}
                    </a></td>
            </tr>
        {{/each}}
    </tbody>
</table>